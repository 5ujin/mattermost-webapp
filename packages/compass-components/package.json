{
  "name": "@mattermost/compass-components",
  "version": "0.1.0",
  "description": "Mattermost Componass Design System common components",
  "homepage": "https://github.com/mattermost/mattermost-webapp/packages/compass-components",
  "author": "Mattermost",
  "license": "Apache-2.0",
  "repository": "github:mattermost/mattermost-webapp",
  "bugs": {
    "url": "https://github.com/mattermost/mattermost-webapp/issues"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./components": "./dist/components/index.js",
    "./components/*": "./dist/components/*.js",
    "./constants": "./dist/constants/index.js",
    "./constants/*": "./dist/constants/*.js",
    "./utilities": "./dist/utilities/index.js",
    "./utilities/*": "./dist/utilities/*.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "start": "tsc --watch",
    "start:storybook": "start-storybook -p 6006",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "build:storybook": "build-storybook",
    "check": "run-s check:lint check:types",
    "check:lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "check:types": "tsc --noEmit",
    "fix": "npm run check:lint -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@mattermost/compass-icons": "^0.1.0",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/react": "^6.1.2",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.1",
    "classnames": "^2.2.6",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "classnames": "^2.2.6"
  }
}
